import java.net.*;
import java.io.*;
import java.util.*;

public class Server {

	/**
	 * @param args
	 */
	public static InetAddress loopBackAddress;
	public static ServerSocket a;
	public static Socket b,c;
	public static void main(String[] args) {
		try{
			
			a = new ServerSocket(1600);
			b = new Socket("loopback", 1600);
			Socket client = a.accept();
			System.out.println(b.getInetAddress());
			a.close();
			b.close();
			client.close();
		
		
			
		} catch(UnknownHostException e){
			System.err.println(e);
		
		} catch (IOException e) {
			System.err.println(e);
		}

	}

}


